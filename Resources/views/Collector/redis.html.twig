{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
<span style="white-space:nowrap; color:#2f2f2f; display:inline-block; min-height:24px; border-right:1px solid #cdcdcd; padding:5px 10px 5px 10px; ">
    <img width="24" height="24" alt="Redis" style="vertical-align: middle; margin-right: 10px;" src="data:image/png;base64,"/>
    <span style="color: {{ collector.commandcount < 10 ? '#000' : '#d22' }}" title="{{ '%0.2f'|format(collector.time) }} ms">{{ collector.commandcount }}</span>
</span>
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img src="{{ asset('bundles/redis/images/redis.png') }}" alt="Redis" /></span>
    <strong>Redis</strong>
    <span class="count">
        <span>{{ collector.commandcount }}</span>
        <span>{{ '%0.0f'|format(collector.time) }} ms</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    <h2>Commands</h2>

    {% if not collector.commandcount %}
        <p><em>No commands.</em></p>
    {% else %}
        <ul class="alt">
            {% for i, command in collector.commands %}
                <li class="{{ i is odd ? 'odd' : 'even' }}">
                    <div>
                        <code>{{ command.cmd }}</code>
                    </div>
                    <small>
                        <strong>Time</strong>: {{ '%0.2f'|format(command.executionMS) }} ms
                        <strong>Connection</strong>: {{ command.conn }}
                    </small>
                </li>
            {% endfor %}
        </ul>
    {% endif %}
{% endblock %}
